                    <physDesc>
                        <objectDesc>
                            <supportDesc>
                                <support>Canmore <ref target="https://canmore.org.uk/site/39479/poltalloch"><idno type="Canmore">39479</idno></ref>
                                    <material>sandstone</material> 
