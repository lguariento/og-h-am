            <div type="edition" subtype="roman" xml:lang="la-Latn" xml:space="preserve" resp="#NW">
                <ab>
                    <lb n="1" style="text-direction:down"/><persName><name nymRef="dunocatus">DV<hi rend="reversed">N</hi>OCATI</name> <w type="formula">HIC</w> <w type="formula">IACIT</w> <w type="formula" lemma="filius">
                        <lb n="2" style="text-direction:down"/><hi rend="ligature">FI</hi>LI</w> <name nymRef="mescagnus">MESCAGNI</name></persName>
                </ab>
