                    <lb n="1"/><persName><w type="formula" lemma="maqqas"><supplied reason="lost" cert="medium">M</supplied>AQI</w> <w type="formula" lemma="avias">AVI</w> <name><g ref="#forfid_X" type="interpretation_K"/>OLUI</name> <w type="formula" lemma="koi?"><g ref="#forfid_X" type="interpretation_K"/>OI</w></persName>
