            <div type="apparatus">
                <listApp>
                <app n="1">
                    <note>O'Kelly and Shee (1968, 43) and McManus (1991, 72-73) read:</note>
                    <rdg>
                    <persName><name><supplied reason="lost">A</supplied>DARUN</name> <w>M<supplied reason="lost">A</supplied>CI</w> <name>COLAL<supplied reason="lost">I</supplied></name></persName>
