                <lb n="1"/><persName><name nymRef="#odaragnas"><unclear reason="damage">A</unclear>DARAGN</name> <w type="formula" lemma="maqqas">M<supplied reason="lost" cert="low">A</supplied>CI</w> <name nymRef="#?">COLAL<supplied reason="lost" cert="low">I</supplied></name></persName>
                </ab>                
