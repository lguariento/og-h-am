                    <lb n="1" style="text-direction:up"/><gap reason="lost" extent="unknown" unit="character"/><g ref="#bound_I"/><g ref="#bound_R"/><unclear reason="ambiguous"><g ref="#bound_U"/></unclear><gap reason="lost" extent="unknown" unit="character"/>
                </ab>
